<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.an"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.an.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".thread.asyntask.AsynTaskExecutorActivity" >
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" >
                </category>
            </intent-filter>
        </activity>
        <activity
            android:name="com.an.pic.blur.GaosiActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_gaosi" >
        </activity>
        <activity
            android:name="com.an.pic.blur.RSActivity"
            android:label="@string/title_activity_rs" >
        </activity>
        <activity
            android:name="com.an.pic.blur.BlurActivity"
            android:label="@string/title_activity_blur" >
        </activity>
         <activity
            android:name="com.an.pic.blur.NativeBlurActivity"
            android:label="@string/native_method" >
        </activity>
    </application>

</manifest>